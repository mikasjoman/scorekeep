layers';\n//my own\nimport TitleBar from '../imports/ui/TitleBar';\nimport AddPlayer from '../imports/ui/AddPlayer';\n\nconst renderPlayers = (players) => {\n  return players.map(player => {\n    return (\n      <div key={player._id}>\n        <p>\n          { player.name } has score {player.score}\n\n          <button onClick={()=> Players.update(\n           player._id ,\n            { $inc: { score: 1 } }\n          )}>+1</button>\n\n          <button\n            onClick={() => Players.remove({_id: player._id}) }\n          >X</button>\n\n          <button onClick={() => Players.update(\n            player._id, { $inc: { score: -1 }}\n          )}>-1</button>\n        </p>\n      </div>\n    );\n  });\n}\n\n\n\nMeteor.startup(() => {\n  Tracker.autorun(() => {\n    var players = Players.find().fetch();\n\n    let jsx = (\n      <div>\n        <TitleBar title=\"Score Keep\"/>\n        {renderPlayers(players)}\n        <AddPlayer />\n      </div>\n    );\n    ReactDOM.render(jsx, document.getElementById('app'));\n  });\n\n});\n","import { Mongo } from 'meteor/mongo';\n\nexport const Players = new Mongo.Collection('players'); // constructor function\n","import React, { Component } from 'react';\nimport { Players } from '../api/players';\n\n\nexport default class AddPlayer extends Component {\n\n  handleSumbit(event) {\n    event.preventDefault();\n    let playerName  = event.target.playerName.value;\n    if (playerName){\n      event.target.playerName.value = \"\"; // reset the form\n      Players.insert({\n        name: playerName,\n        score: 0\n      });\n    }\n  };\n\n  render() {\n    return (\n      <form onSubmit={this.handleSumbit}>\n        <input type=\"text\" name=\"playerName\" placeholder=\"Player name\"/>\n        <button>Add player</button>\n      </form>\n    );\n  }\n}\n","import React, { Component } from 'react';\n\nexport default class TitleBar extends Component {\n  render() {\n    return (\n      <div><h1>{this.props.title}</h1></div>\n    );\n  }\n}\n\nTitleBar.propTypes = {\n  title: React.PropTypes.string.isRequired\n}\nTitleBar.defaultProps = {\n  title: 'Default title'\n}\n"]}}]