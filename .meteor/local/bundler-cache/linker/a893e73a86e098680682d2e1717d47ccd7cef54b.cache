er => {\n    return (\n      <div key={player._id}>\n        <p>\n          { player.name } has score {player.score}\n\n          <button onClick={()=> Players.update(\n            { _id: player._id },\n            { $inc: { score: 1 } }\n          )}>+1</button>\n\n          <button\n            onClick={() => Players.remove({_id: player._id}) }\n          >X</button>\n\n          <button onClick={() => Players.update(\n            { _id: player._id },\n            { $inc: { score: -1 }}\n          )}>-1</button>\n        </p>\n      </div>\n    );\n  });\n}\n\n\nconst handleSumbit = (event) => {\n  event.preventDefault();\n  let playerName  = event.target.playerName.value;\n  if (playerName){\n    event.target.playerName.value = \"\"; // reset the form\n    Players.insert({\n      name: playerName,\n      score: 0\n    });\n  }\n};\n\nMeteor.startup(() => {\n  Tracker.autorun(() => {\n    var players = Players.find().fetch();\n\n    let jsx = (\n      <div>Hello\n      {renderPlayers(players)}\n      <form onSubmit={handleSumbit}>\n        <input type=\"text\" name=\"playerName\" placeholder=\"Player name\"/>\n        <button>Add player</button>\n      </form>\n      </div>\n    );\n    ReactDOM.render(jsx, document.getElementById('app'));\n  });\n\n});\n","import { Mongo } from 'meteor/mongo';\n\nexport const Players = new Mongo.Collection('players'); // constructor function\n"]}}]