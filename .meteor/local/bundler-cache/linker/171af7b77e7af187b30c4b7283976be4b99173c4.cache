,WAAST,MAAMkC,SAAN,CAAgBG,KAAhB,CAAsBD;AADV,CAAvB,yH;;;;;;;;;;;;;;ACxBA,OAAOpC,KAAP,IAAgBC,SAAhB,QAAiC,OAAjC;;IAEqB4B,Q;;;;;;;;;qBAEnBoB,c;8BAAiB;AACf,UAAG,KAAKlB,KAAL,CAAWmB,QAAd,EAAwB;AACtB,eAAO;AAAA;AAAA;AAAK,eAAKnB,KAAL,CAAWmB;AAAhB,SAAP;AACD;AACF;;;;;qBACDpC,M;sBAAS;AACP,aACE;AAAA;AAAA,UAAK,WAAU,WAAf;AACE;AAAA;AAAA,YAAK,WAAU,SAAf;AACE;AAAA;AAAA;AAAK,iBAAKiB,KAAL,CAAWC;AAAhB,WADF;AAEG,eAAKiB;AAFR;AADF,OADF;AAQD;;;;;;EAhBmChD,S;;eAAjB4B,Q;;;AAmBrBA,SAASI,SAAT,GAAqB;AACnBD,SAAOhC,MAAMkC,SAAN,CAAgBC,MAAhB,CAAuBC;AADX,CAArB;AAGAP,SAASsB,YAAT,GAAwB;AACtBnB,SAAO;AADe,CAAxB,yH","file":"/app.js","sourcesContent":["import React, { Component } from 'react';\nimport ReactDOM from 'react-dom';\nimport { Meteor } from 'meteor/meteor';\nimport { Tracker } from 'meteor/tracker'\n\nimport { Players } from '../imports/api/players';\n//my own\nimport App from '../imports/ui/App';\n\nMeteor.startup(() => {\n  Tracker.autorun(() => {\n    var players = Players.find({}, {\n        sort: {\n          score: 1\n        }\n      }).fetch();\n\n    ReactDOM.render(<App players={players} title=\"Hello\" />, document.getElementById('app'));\n  });\n\n});\n","import { Mongo } from 'meteor/mongo';\n\nexport const Players = new Mongo.Collection('players'); // constructor function\n","import React, { Component } from 'react';\nimport { Players } from '../api/players';\n\n\nexport default class AddPlayer extends Component {\n\n  handleSumbit(event) {\n    event.preventDefault();\n    let playerName  = event.target.playerName.value;\n    if (playerName){\n      event.target.playerName.value = \"\"; // reset the form\n      Players.insert({\n        name: playerName,\n        score: 0\n      });\n    }\n  };\n\n  render() {\n    return (\n      <form onSubmit={this.handleSumbit.bind(this)}>\n        <input type=\"text\" name=\"playerName\" placeholder=\"Player name\"/>\n        <button>Add player</button>\n      </form>\n    );\n  }\n}\n","import React, { Component } from 'react';\n\nimport TitleBar from './TitleBar';\nimport AddPlayer from './AddPlayer';\nimport PlayerList from './PlayerList';\n\nexport default class App extends Component {\n\n  render() {\n    return (\n      <div>\n        <TitleBar title={this.props.title}/>\n        <div className=\"wrapper\">\n          <PlayerList players={this.props.players}/>\n          <AddPlayer />\n        </div>\n      </div>\n    );\n  }\n}\n\nApp.propTypes ={\n  title: React.PropTypes.string.isRequired,\n  players: React.PropTypes.array.isRequired\n}\n","import React, { Component } from 'react';\nimport { Players } from '../api/players';\n\n\nexport default class Player extends Component {\n\n  render() {\n    let { player, key } = this.props;\n\n    return (\n      <div key={this.props.key}>\n        <p>\n          { player.name } has score {player.score}\n\n          <button onClick={()=> Players.update(\n           player._id ,\n            { $inc: { score: 1 } }\n          )}>+1</button>\n\n          <button\n            onClick={() => Players.remove({_id: player._id}) }\n          >X</button>\n\n          <button onClick={() => Players.update(\n            player._id, { $inc: { score: -1 }}\n          )}>-1</button>\n        </p>\n      </div>\n    );\n  }\n}\n\nPlayer.propTypes ={\n  player : React.PropTypes.object.isRequired\n}\n","import React, { Component } from 'react';\n\nimport Player from './Player';\n\nexport default class PlayerList extends Component {\n\n  renderPlayers() {\n    if(this.props.players.length === 0){\n      return <p>No players in db</p>\n    }\n    return this.props.players.map(player => {\n        return <Player key={player._id} player={player}/>\n    });\n  }\n\n  render() {\n    return (\n      <div>\n        {this.renderPlayers()}\n      </div>\n    );\n  }\n}\n\nPlayerList.propTypes = {\n  players: React.PropTypes.array.isRequired\n}\n","import React, { Component } from 'react';\n\nexport default class TitleBar extends Component {\n\n  renderSubTitle() {\n    if(this.props.subTitle) {\n      return <h2>{this.props.subTitle}</h2>;\n    }\n  }\n  render() {\n    return (\n      <div className=\"title-bar\">\n        <div className=\"wrapper\">\n          <h1>{this.props.title}</h1>\n          {this.renderSubTitle}\n        </div>\n      </div>\n    );\n  }\n}\n\nTitleBar.propTypes = {\n  title: React.PropTypes.string.isRequired\n}\nTitleBar.defaultProps = {\n  title: 'Default title'\n}\n"]}}]